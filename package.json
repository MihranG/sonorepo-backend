  "description": "SonoFlow Backend API - Queue Management & Report Generation for Sonography Clinics",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "debug": "nodemon --inspect=9229 --exec ts-node server.ts",
    "init-db": "ts-node scripts/initDb.ts",
